{
  "experiment_id": "",
  "device": "gpu",
  "experiment_type": "train",
  "experiment_folder": "./experiments/FullSimRNN",
  "model_class": "model.RNN.FullSimRNN",
  "model_hparams": {
    "rnn_encoder_class": "model.RNN.UAVRNNEncoder",
    "rnn_encoder_hparams": {
      "num_layers": 1,
      "hidden_size": 256,
      "bidirectional": "True"
    },
    "agg_seq_states": "True",
    "fc_hparams": {
      "hidden_sizes": [1024, 512, 256]
    },
    "reg_hparams": {
      "hidden_sizes": [128, 64, 16]
    },
    "clf_hparams": {
      "hidden_sizes": [128, 32, 8]
    },
    "use_embed": "True",
    "embedding_size": 32
  },
  "dropout": 0.0,
  "num_outcomes": 1,
  "optimizer_class": "torch.optim.AdamW",
  "optimizer_hparams": {
    "lr": 0.001
  },
  "scheduler_class": "torch.optim.lr_scheduler.MultiStepLR",
  "scheduler_hparams": {
    "milestones":  [5],
    "gamma": 0.1
  },
  "dataloader_hparams": {
    "batch_size": 256,
    "shuffle": "True"
  },
  "dataset_class": "data.UAVDataset.UAVRegressionDataset",
  "dataset_hparams": {
    "datafile": "data/datafiles/preprocessed/filtered_aggregated_uav_designs.csv",
    "target_cols": ["range", "cost", "velocity", "result"],
    "scale": "True",
    "scaler_class": "sklearn.preprocessing.RobustScaler",
    "one_hot": "False"
  },
  "val_proportion": 0.20,
  "test_proportion": 0.20,
  "loss_class": "train.Loss.MAEBCELoss",
  "loss_hparams": {
  },
  "loggers": [
    {
      "logger_class": "train.Logging.ConsoleLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.DatafileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
      {
      "logger_class": "train.Logging.FileLogger",
      "logger_hparams": {
        "mode": 1
      }
    }
  ],
  "stoppers": [
    {
      "stopper_class": "train.TrainStopper.EpochStopper",
      "stopper_hparams": {
        "stop_epoch": 200
      }
    },
    {
      "stopper_class": "train.TrainStopper.PlateauStopper",
      "stopper_hparams": {
        "patience": 50,
	"start_epoch": 50
      }
    }
  ],
  "simulator_class": "inference.UAVSimulator.NNSimulator",
  "simulator_hparams": {
    "simulator_results_file": "sim_results.csv"
  },
  "postprocessor_class": "data.DataPostprocessor.UAVDataPostprocessor"
}
