{
  "hparams_file": "nn_hparams.json",
  "experiment_id": "",
  "device": "gpu",
  "experiment_type": "train",
  "experiment_folder": "./experiments/SimNN",
  "model_class": "model.NN.MHSimNN",
  "feature_dim": 295,
  "model_hparams": {
    "fc_hparams": {
      "hidden_sizes": [1024, 2048, 1024]
    },
    "reg_hparams": {
      "hidden_sizes": [512, 512, 256, 128]
    },
    "clf_hparams": {
      "hidden_sizes": [512, 128, 32, 8]
    }
  },
  "dropout": 0.0,
  "num_outcomes": 1,
  "optimizer_class": "torch.optim.Adam",
  "optimizer_hparams": {
    "lr": 0.001
  },
  "scheduler_class": "torch.optim.lr_scheduler.MultiStepLR",
  "scheduler_hparams": {
    "milestones":  [20, 1000],
    "gamma": 0.1
  },
  "dataloader_hparams": {
    "batch_size": 256,
    "shuffle": "True"
  },
  "dataset_class": "data.UAVDataset.UAVMatrixDataset",
  "dataset_hparams": {
    "datafile": "data/datafiles/preprocessed/design_database_april.csv",
    "target_cols": ["range", "cost", "velocity", "result"],
    "scale": "True",
    "scaler_class": "sklearn.preprocessing.RobustScaler",
    "encoding_scheme": "matrix"
  },
  "n_folds": 4,
  "val_proportion": 0.2,
  "test_proportion": 0.1,
  "save_testset": false,
  "loss_class": "train.Loss.WeightedMAEBCELoss",
  "loss_hparams": {
    "pos_weight": 1
  },
  "loggers": [
    {
      "logger_class": "train.Logging.ConsoleLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.DatafileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
      {
      "logger_class": "train.Logging.FileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.WandbLogger",
      "logger_hparams": {
        "mode": 1,
        "project_name": "uav-design"
      }
    }
  ],
  "stoppers": [
    {
      "stopper_class": "train.TrainStopper.EpochStopper",
      "stopper_hparams": {
        "stop_epoch": 5000
      }
    },
    {
      "stopper_class": "train.TrainStopper.PlateauStopper",
      "stopper_hparams": {
        "patience": 20,
	    "start_epoch": 100
      }
    }
  ],
  "simulator_class": "inference.NNSimulator.NNSimulator",
  "simulator_hparams": {
    "simulator_results_file": "sim_results.csv"
  },
  "postprocessor_class": "data.DataPostprocessor.UAVMatrixPostprocessor"
}
