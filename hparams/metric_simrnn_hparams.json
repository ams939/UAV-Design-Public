{
  "experiment_id": "",
  "device": "gpu",
  "experiment_type": "train",
  "experiment_folder": "./experiments/MetricSimRNN",
  "model_class": "model.RNN.MetricSimRNN",
  "model_hparams": {
    "rnn_encoder_class": "model.RNN.UAVRNNEncoder",
    "rnn_encoder_hparams": {
      "num_layers": 1,
      "hidden_size": 256,
      "bidirectional": "True"
    },
    "agg_seq_states": "True",
    "fc_hparams": {
      "fc_hidden_sizes": [256, 128, 128]
    },
    "use_embed": "True",
    "embedding_size": 32
  },
  "dropout": 0.2,
  "num_outcomes": 1,
  "optimizer_class": "torch.optim.AdamW",
  "optimizer_hparams": {
    "lr": 0.001
  },
  "scheduler_class": "torch.optim.lr_scheduler.MultiStepLR",
  "scheduler_hparams": {
    "milestones":  [10, 200],
    "gamma": 0.1
  },
  "dataloader_hparams": {
    "batch_size": 64,
    "shuffle": "True"
  },
  "dataset_class": "data.UAVDataset.UAVRegressionDataset",
  "dataset_hparams": {
    "datafile": "data/datafiles/preprocessed/test.csv",
    "target_cols": ["range", "cost", "velocity"],
    "scale": "True",
    "scaler_class": "sklearn.preprocessing.RobustScaler",
    "one_hot": "False"
  },
  "val_proportion": 0.25,
  "loss_class": "train.Loss.CustomMSELoss",
  "loss_hparams": {
  },
  "loggers": [
    {
      "logger_class": "train.Logging.ConsoleLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.DatafileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
      {
      "logger_class": "train.Logging.FileLogger",
      "logger_hparams": {
        "mode": 1
      }
    }
  ],
  "stoppers": [
    {
      "stopper_class": "train.TrainStopper.EpochStopper",
      "stopper_hparams": {
        "stop_epoch": 1200
      }
    },
    {
      "stopper_class": "train.TrainStopper.PlateauStopper",
      "stopper_hparams": {
        "patience": 25,
	"start_epoch": 50
      }
    }
  ],
  "simulator_class": "inference.UAVSimulator.NNSimulator",
  "simulator_hparams": {
    "simulator_results_file": "sim_results.csv"
  },
  "postprocessor_class": "data.DataPostprocessor.UAVDataPostprocessor",
  "save_preds": "True"
}
