{
  "seed": 42,
  "experiment_folder": "./experiments/DQN",
  "memory": "",
  "model_file": "",
  "val_memory": "experiments/DQN/val_memory.pkl",
  "model_class": "model.DQN.UAVDQN",
  "model_hparams": {
    "fc_hparams": {
      "hidden_sizes": [512, 256, 128]
    },
    "noisy_fc_hparams": {
      "hidden_size": 64,
      "noisy_std": 0.1
    }
  },
  "e_init": 0.5,
  "e_end": 0.05,
  "episode_offset": 0,
  "encoding": "matrix",
  "dynamic_termination": false,
  "experiment_id": "",
  "T_max": 30000,
  "max_episode_length": 25,
  "history_length": 1,
  "atoms": 51,
  "V_min": -1,
  "V_max": 1,
  "memory_capacity": 10000,
  "replay_frequency": 4,
  "priority_exponent": 0.5,
  "priority_weight": 0.4,
  "multi_step": 3,
  "discount": 0.99,
  "target_update": 100,
  "reward_clip": 2,
  "learning_rate": 0.001,
  "adam_eps": 0.00015,
  "batch_size": 32,
  "norm_clip": 10,
  "learn_start": 1000,
  "evaluate": false,
  "evaluation_interval": 1000,
  "evaluation_episodes": 10,
  "evaluation_size": 500,
  "enable_cudnn": true,
  "checkpoint_interval": 500,
  "disable_bzip_memory": true,
  "loggers": [
    {
      "logger_class": "train.Logging.ConsoleLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.FileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.WandbLogger",
      "logger_hparams": {
        "mode": 1,
        "project_name": "uav-design"
      }
    },
    {
      "logger_class": "train.Logging.DatafileLogger",
      "logger_hparams": {
        "mode": 1
      }
    },
    {
      "logger_class": "train.Logging.PlotLogger",
      "logger_hparams": {
        "mode": 1,
        "x_label": "Episode",
        "y_label": "Average reward",
        "title": "Average reward vs. episode"
      }
    }
  ],
  "simulator_class": "inference.UAVSimulator.HyFormSimulator",
  "agg_seqs": false,
  "reward_class": "rl.Reward.DeltaReward",
  "reward_hparams": {
    "range_weight": 0.333,
    "cost_weight": 0.333,
    "velocity_weight": 0.333,
    "early_terminate": true,
    "objective": "1a",
    "objective_type": "pre-defined"
  },
  "init_state": "*aMM0++++,15,3",
  "save_mem": false,
  "no_size": true,
  "symmetric_actions": true
}
